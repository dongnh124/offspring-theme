<!-- header-13.liquid -->
<header class="site-header {% if section.settings.enable_sticky %}site-header--sticky{% endif %} {% if section.settings.enable_transparent %}site-header--transparent{% endif %}" role="banner" data-section-id="{{ section.id }}" data-section-type="header">
  <div class="{% if template == 'index' %}container {% endif %}">
    <div class="header-top">
      <div id="topbar" class="topbar">
        <div class="row no-gutters">
          {%- if section.settings.topbar_left != blank -%}
          <div class="col-9 col-lg topbar-col-left text-left">
            <span>
              {{section.settings.topbar_left}}
            </span>
          </div>
          {%- endif -%}
          <div class="col-3 col-lg topbar-col-right text-right">
            {% for block in section.blocks %}
            <span class="topbar-link">
              <a href="{{block.settings.link}}" title="{{block.settings.title}}">
                {{block.settings.title}}
              </a>
            </span>
            {% endfor %}
            <span class="topbar-link">
              {%- include 'block-currencies' -%}
            </span>
          </div>
        </div>
      </div>

      <div class="{% if template != 'index' %}container {% endif %}">
        <div class="row no-gutters">
          <div class="col-12 col-lg-2-4 d-flex justify-content-center align-items-center col-left my-3 my-lg-0">
            {%- include 'site-logo' -%}
          </div>
          <div class="col-12 col-lg-9-6 col-right">
            <div class="row row-header">
              <div class="col-12 col-md-9 col-menu">
                {%- include 'block-menu' -%}
              </div>

              <div class="col-12 col-lg-3 d-flex align-items-center justify-content-center justify-content-md-end">
                {%- if shop.customer_accounts_optional or shop.customer_accounts_enabled -%}
                {%- if customer  -%}
                <a href="/account" title="{{'customer.account.title' | t}}" class="site-header__link site-account text-hide">
                  {%- include 'icon-user' -%}
                  {{'customer.account.title' | t}}
                </a>
                <a href="/account/logout" title="{{'layout.customer.log_out' | t}}" class="site-header__link site-logout text-hide">
                  {%- include 'icon-sign-out' -%}
                  {{'layout.customer.log_out' | t}}
                </a>
                {%- else -%}
                {%- if template != 'customers/login' -%} {%- comment DOES NOT APPEAR ON LOGIN PAGE - AVOID CONFLICT -%} {%- endcomment -%}
                <a href="javascript:void(0)" data-toggle="modal" data-target="#jsLogin" class="site-header__link site-account js-open-login text-hide">
                  <span title="{{'customer.login.sign_in' | t}}">{%- include 'icon-user' -%}</span>
                  {{'customer.login.sign_in' | t}}
                </a>
                {%- endif -%}
                {%- endif -%}
                {%- endif -%}
                {%- include 'block-wishlist' -%}
                {%- include 'block-compare' -%}
                {%- include 'block-cart' -%}
                <div class="d-inline-block d-lg-none toggle-menu-mobile">
                  {%- include 'button-toggle-menu-mobile' -%}
                </div>
              </div>
            </div>

            <div class="row"> 
              <div class="col-12 d-none d-md-flex">
                <div class="site-header__search-wrap">
                  <form id="form-search-header" action="/search" method="get" class="site-header__search js-ajaxsearch" role="search">
                    <span class="form-search-icon">{%- include 'icon-search' -%}</span>
                    <input type="hidden" name="type" value="product">
                    <input onfocus="this.value=''" type="search" name="q" id="SiteNavSearch" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" autocomplete="off" class="site-header__search-input">
                    <button type="submit" class="text-link site-header__search-submit">
                      {%- include 'icon-search' -%}
                      <span>{{ 'general.search.submit' | t }}</span>
                    </button>
                    <div class="search-results"></div>
                  </form>
                </div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</header>
